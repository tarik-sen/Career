@page
@model Career.Areas.Admin.Pages.Jobs.Manage.DetailsModel
@{
    ViewData["Title"] = "Job Details";
    ViewData["ActivePage"] = "Details";
}


<partial name="_JobDetails" model="Model.JobModel"/>


@section Scripts {
    <script type="text/javascript">
        $(document).ready(function () {
            SetApplicantNumber($(".job-container").attr("id").split("-").pop(), "#applicant-number");
        });
    </script>
}