<div id="sidebar" class="d-flex flex-column flex-shrink-0 p-3 sidebar border-end border-1 ">
    <button id="sidebar-collapse" class="btn btn-lg d-flex align-items-center ps-3 text-dark" type="button">
        <svg class="bi bi-list" width="24" height="24"><use xlink:href="#toggle-icon" /></svg>
        <span class="sidebar-collapsible sidebar-header">Career</span>
    </button>
    
    <hr/>

    <ul class="nav nav-pills flex-column">
        <li class="nav-item">
            <a class="nav-link @ProfileSidebarPages.OverviewNav(ViewContext)" id="profile-overview" asp-page="./Index">
                <svg class="bi bi-person-vcard" width="24" height="24"><use xlink:href="#overview-icon" /></svg>
                <span class="sidebar-collapsible">CV Overview</span>
            </a>
        </li>
        <li class="nav-item">
            <a class="nav-link @ProfileSidebarPages.ContactNav(ViewContext)" id="profile-contact" asp-page="./Contact">
                <svg class="bi bi-telephone" width="24" height="24"><use xlink:href="#contact-icon" /></svg>
                <span class="sidebar-collapsible">Contact</span>
            </a>
        </li>
        <li class="nav-item">
            <a id="shit" class="nav-link @ProfileSidebarPages.PersonalNav(ViewContext)" id="profile-personal" asp-page="./Personal">
                <svg class="bi bi-person" width="24" height="24"><use xlink:href="#personal-icon" /></svg>
                <span class="sidebar-collapsible">Personal</span>
            </a>
        </li>
        <li class="nav-item">
            <a class="nav-link @ProfileSidebarPages.ExperiencesNav(ViewContext)" id="profile-experiences" asp-page="./Experiences">
